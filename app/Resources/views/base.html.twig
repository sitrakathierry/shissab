</head>

<body>
<div id="wrapper">

    {{ render(controller('MenuBundle:Default:index')) }}

    <div id="page-wrapper" class="gray-bg dashbard-1">
        {{ include ('MenuBundle:Default:menu-top.html.twig') }}

        <div class="row">
                <div class="col-lg-12">
                    <div class="wrapper wrapper-content">
                        {% block contenu %}{% endblock %}
                    </div>
                </div>
        </div>
    </div>

    <div id="modal-ui" data-id="0"></div>

    <div class="modal inmodal" id="modal" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog" id="modal-size">
            <div class="modal-content animated" id="modal-animated">
                <div class="modal-header deplacer">
                    <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                    <h5 class="modal-title" id="modal-header">Modal title</h5>
                </div>
                <div class="modal-body" id="modal-body" style="height: 100%!important;">...</div>
                <div class="modal-footer hidden" id="modal-footer"></div>
            </div>
        </div>
    </div>
</div>

