{% extends 'base.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" href="{{ asset('css/plugins/jqGrid/ui.jqgrid.css') }}">
    <style type="text/css">
        .ui-jqgrid tr.jqgrow {
            height: 50px !important;
        }
    </style>
 {% endblock  %}

 {% block scripts %}
    {{ parent() }}
    <script src="{{ asset('js/plugins/jqGrid/jquery.jqGrid.min.js') }}"></script>
    <script src="{{ asset('bundles/agence/js/list.js') }}"></script>
{% endblock %}

{% block contenu %}
    <div class="wrapper wrapper-content">
        <div class="row">
        	<div class="col-lg-12">
                <div class="ibox float-e-margins">
                    <div class="ibox-title">
                        <h5 style="font-size:20px">
                            <i class="fa fa-home"></i>
                            LISTE DES AGENCES
                        </h5>
                        <div class="ibox-tools">
                            <a href="{{ path('agence_add') }}" class="btn btn-primary btn-xs" type="reset">
                                <i class="fa fa-plus"></i>
                                CREATION
                            </a>
                        </div>
                    </div>
                    <div class="ibox-content">
                    	<table id="list_agence"></table>
                    </div>
                </div>
            </div>
        </div>
    </div>
{% endblock %}

